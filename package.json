    "cors": "2.8.5",
    "dayjs": "^1.8.19",
    "finalhandler": "1.1.2",
    "jsonwebtoken": "8.5.1",
    "markdown-it": "10.0.0",
    "micro": "9.3.4",
    "node-cache": "5.1.0",
